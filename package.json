{
  "name": "react-stuttgart",
  "version": "1.0.0",
  "description": "Website for #ReactStuttgart meetup",
  "scripts": {
    "start": "NODE_ENV=development babel-node src/server.js",
    "stack:deploy": "docker stack deploy -c .docker/development/docker-compose.yml reactstuttgart",
    "stack:remove": "docker stack rm reactstuttgart",
    "prebuild": "rimraf ./build && mkdir build",
    "build": "NODE_ENV=production babel-node scripts/build.js",
    "postbuild": "NODE_ENV=production babel src/server.js --out-file build/server.js",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "jest",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "lint": "eslint --ext .js src/**/*; exit 0;",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "server.js",
  "repository": {
    "url": "https://github.com/soosap/react-stuttgart.git",
    "type": "git"
  },
  "author": "Prasath Soosaithasan <prasath.soosaithasan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@saronia/saronia-ui": "^0.3.0",
    "axios": "^0.16.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "contentful": "^3.8.1",
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "marked": "^0.3.6",
    "meetup-api": "github:soosap/meetup-api#18195c1",
    "normalizr": "^3.2.2",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-inlinesvg": "^0.6.0",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.4",
    "redbox-react": "^1.3.6",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-form-saga": "^0.1.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-saga": "^0.15.3",
    "regenerator-runtime": "^0.10.1",
    "reselect": "^3.0.0",
    "styled-components": "next"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.1",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.46.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "jest-cli": "^18.0.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.2",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4",
    "semantic-ui-css": "^2.2.9",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.js"
    }
  }
}
