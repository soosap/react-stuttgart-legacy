{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Dockerized React Starter Kit",
  "scripts": {
    "start": "npm run compose:up",
    "compose:up": "docker-compose -f docker/development/docker-compose.yml up",
    "compose:down": "docker-compose -f docker/development/docker-compose.yml down",
    "share": "npm-run-all --parallel start localtunnel",
    "security-check": "nsp check",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 8080",
    "test": "docker-compose -f docker/test/docker-compose.yml up --build test",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "generate": "plop --plopfile generators/index.js",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist",
    "prbuilder": "npm-run-all clean-dist test lint",
    "build": "env-cmd .env cross-env NODE_ENV=production babel-node scripts/build.js",
    "image:build": "env-cmd .env cross-env NODE_ENV=production ./scripts/buildDockerImage.sh",
    "image:publish": "env-cmd .env cross-env NODE_ENV=production ./scripts/publishDockerImage.sh",
    "semantic:build": "cd semantic && gulp build"
  },
  "main": "server.js",
  "repository": {
    "url": "https://github.com/saronia/react-starter-kit.git",
    "type": "git"
  },
  "author": "Prasath Soosaithasan <prasath.soosaithasan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-cli": "^6.18.0",
    "chalk": "^1.1.3",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cross-env": "^3.1.3",
    "env-cmd": "^4.0.0",
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "recompose": "^0.21.0",
    "redux": "^3.6.0",
    "redux-form": "^6.4.0",
    "redux-form-saga": "^0.0.8",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-saga": "^0.13.0",
    "regenerator-runtime": "^0.10.1",
    "semantic-ui": "^2.2.7",
    "styled-components": "^1.2.1"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "jest-cli": "^18.0.0",
    "localtunnel": "^1.8.2",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "plop": "^1.7.3",
    "react-addons-test-utils": "^15.4.1",
    "redbox-react": "^1.3.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.js"
    }
  }
}
