{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "Dockerized React Starter Kit",
  "scripts": {
    "start": "env-cmd .env cross-env NODE_ENV=development babel-node server.js",
    "share": "npm-run-all --parallel start localtunnel",
    "security-check": "nsp check",
    "localtunnel": "lt --port 8080",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "jest:coverage": "jest --coverage",
    "test": "jest",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "clean-build": "rimraf ./build && mkdir build",
    "prebuild": "npm-run-all clean-build test",
    "build": "env-cmd .env cross-env NODE_ENV=production babel-node scripts/build.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "main": "server.js",
  "repository": {
    "url": "https://github.com/soosap/react-stuttgart.git",
    "type": "git"
  },
  "author": "Prasath Soosaithasan <prasath.soosaithasan@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "contentful": "^3.8.1",
    "cross-env": "^3.1.3",
    "env-cmd": "^4.0.0",
    "express": "^4.14.0",
    "marked": "^0.3.6",
    "meetup-api": "github:soosap/meetup-api#18195c1",
    "regenerator-runtime": "^0.10.1"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@saronia/saronia-ui": "^0.1.0",
    "axios": "^0.16.1",
    "babel-core": "^6.20.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.9",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.5",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.41.0",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.1.0",
    "immutable": "^3.8.1",
    "jest-cli": "^18.0.0",
    "json-loader": "^0.5.4",
    "localtunnel": "^1.8.2",
    "normalizr": "^3.2.2",
    "npm-run-all": "^3.1.2",
    "nsp": "^2.6.2",
    "ramda": "^0.23.0",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-inlinesvg": "^0.6.0",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.1",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-form": "^6.6.3",
    "redux-form-saga": "^0.1.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-saga": "^0.15.3",
    "reselect": "^3.0.0",
    "rimraf": "^2.5.4",
    "semantic-ui-css": "^2.2.9",
    "semantic-ui-react": "^0.68.2",
    "style-loader": "^0.13.1",
    "styled-components": "^1.4.6",
    "url-loader": "^0.5.7",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-md5-hash": "^0.0.5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/scripts/fileMock.js"
    }
  }
}
